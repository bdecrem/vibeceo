{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2020",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"outDir": "./dist",
		"rootDir": "./",
		"strict": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"allowJs": true,
		"resolveJsonModule": true,
		"noEmit": false
	},
	"include": ["scripts/**/*", "lib/discord/**/*", "src/**/*", "data/**/*"],
	"exclude": ["node_modules", "dist"]
}
