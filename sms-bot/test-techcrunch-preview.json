{
  "definition": {
    "metadata": {
      "name": "TechCrunch AI News",
      "slug": "techcrunch-ai",
      "description": "Latest AI news from TechCrunch artificial intelligence category",
      "category": "technology",
      "version": "1.0.0"
    },
    "triggers": {
      "commands": [
        {
          "keyword": "TECH AI",
          "description": "Get TechCrunch AI news"
        }
      ]
    },
    "sources": [
      {
        "id": "techcrunch_ai",
        "kind": "builtin",
        "sourceType": "rss",
        "feedUrl": "https://techcrunch.com/category/artificial-intelligence/feed/",
        "maxItems": 5
      }
    ],
    "collation": {
      "strategy": "merge",
      "maxTotalItems": 5
    },
    "pipeline": [
      {
        "kind": "fetch"
      },
      {
        "kind": "sort",
        "sortBy": "publishedAt",
        "order": "desc"
      },
      {
        "kind": "filter",
        "maxItems": 3
      }
    ],
    "output": {
      "sms": {
        "enabled": true,
        "template": "{{agentName}}\n\n{{count}} items:\n\n{{#each items}}{{@index}}. {{title}}\n   {{url}}\n\n{{/each}}",
        "maxLength": 1600
      }
    }
  },
  "context": {
    "dryRun": true,
    "skipNotifications": true
  }
}
